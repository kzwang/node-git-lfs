{
    "rules": {
        "indent": [
            1,
            4,
            {"SwitchCase": 1}
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": [
          1,
          {"vars": "all", "args": "none"}
        ],
        "no-empty": [
          1
        ],
        "no-console": [
          1
        ]
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": false
    },
    "extends": "eslint:recommended"
}