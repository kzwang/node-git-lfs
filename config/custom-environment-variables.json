{
  "base_url": "LFS_BASE_URL",
  "port": "LFS_PORT",
  "private": "LFS_PRIVATE",
  "jwt": {
    "algorithm": "LFS_JWT_ALGORITHM",
    "secret": "LFS_JWT_SECRET",
    "issuer": "LFS_JWT_ISSUER",
    "expiresIn": "LFS_JWT_EXPIRES"
  },
  "store": {
    "type": "LFS_STORE_TYPE",
    "options": {
      "access_key": "AWS_ACCESS_KEY",
      "secret_key": "AWS_SECRET_KEY",
      "bucket": "LFS_STORE_S3_BUCKET",
      "endpoint": "LFS_STORE_S3_ENDPOINT",
      "region": "LFS_STORE_S3_REGION",
      "storage_class": "LFS_STORE_S3_STORAGE_CLASS",
	  "azure_storage_account": "AZURE_STORAGE_ACCOUNT",
	  "azure_storage_access_key": "AZURE_STORAGE_ACCESS_KEY",
	  "azure_storage_connection_string": "AZURE_STORAGE_CONNECTION_STRING",
	  "azure_storage_container": "AZURE_STORAGE_CONTAINER",
      "grid_connection": "LFS_STORE_GRID_CONNECTION"
    }
  },
  "authenticator": {
    "type": "LFS_AUTHENTICATOR_TYPE",
    "options": {
      "username": "LFS_AUTHENTICATOR_USERNAME",
      "password": "LFS_AUTHENTICATOR_PASSWORD",
      "client_public_key": "LFS_AUTHENTICATOR_CLIENT_PUBLIC_KEY",
	  "client_public_key_path": "LFS_AUTHENTICATOR_CLIENT_PUBLIC_KEY_PATH"
    }
  },
  "ssh": {
    "enabled": "LFS_SSH_ENABLED",
    "port": "LFS_SSH_PORT",
    "ip": "LFS_SSH_IP",
    "key": {
      "public": "LFS_SSH_PUBLIC_KEY",
      "private": "LFS_SSH_PRIVATE_KEY"
    }
  }
}
